<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="#5CA6E3B5"
                android:centerColor="#5CA6E3B5"
                android:centerY="0.5"
                android:endColor="#5CA6E3B5"
                android:angle="0"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="?colorPrimary"
                    android:centerColor="?colorPrimary"
                    android:centerY="0.5"
                    android:endColor="?colorPrimary"
                    android:angle="0"
                    />
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress"
        >
        <clip>
            <shape>
                <gradient
                    android:startColor="?colorPrimaryDark"
                    android:endColor="?colorPrimaryDark"
                    android:angle="0" />
            </shape>
        </clip>
    </item>

</layer-list>